{
  "name": "@app/library",
  "version": "0.6.0",
  "private": false,
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/__types__/index.d.ts",
  "scripts": {
    "rm.assets": "rm -rf @types",
    "ready.assets": "run-s  ready:*",
    "ready:definitive": "cpx \"../../../types/**/*.*\" ./@types",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "npm:reset": "rm -rf node_modules && rm yarn.lock",
    "test.lib.pending": "jest --config jest.config.lib.json --silent",
    "build:reset": "rm -rf dist",
    "build:clean": "yarn build:reset",
    "build:types": "rm -rf dist/__types__ && tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "yarn build:types && yarn build:js",
    "build-lib": "yarn build:clean && yarn build:all",
    "postinstall.pending": "yarn build-lib",
    "build.current": "yarn build-lib"
  },
  "dependencies": {
    "@app/types": "*",
    "@app/tools": "*",
    "@types/query-string": "6.1.0",
    "query-string": "6.1.0",
    "slugify": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "11.10.4",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/runtime": "7.4.3",
    "@babel/polyfill": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@types/react-test-renderer": "16.8.1",
    "react-test-renderer": "16.7.0",
    "typescript": "3.0.3"
  }
}

{
    "name": "@app/tools",
    "version": "0.6.0",
    "private": false,
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/__types__/index.d.ts",
    "scripts": {
        "depenence.update": "ncu -u",
        "rm.assets": "rm -rf @types",
        "ready.assets": "run-s  ready:*",
        "ready:definitive": "cpx \"../../../types/**/*.*\" ./@types",
        "ready:copy_static": "cpx \"../../appWeb/app/static/**/*.*\" ./static",
        "ready.post_assets": "run-s  ready_post:*",
        "ready_post:remove_plex": "rm -rf static/plex",
        "test.global": "jest --env=node --colors --coverage test",
        "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
        "build.copy.tld": "cpx \"./src/vendor/parse-domain/tld.js\" ./dist/vendor/parse-domain",
        "build:post": "yarn build.copy.tld",
        "build:reset": "rm -rf dist",
        "build:clean": "yarn build:reset",
        "build:types": "rm -rf dist/__types__ && tsc --emitDeclarationOnly -p tsconfig.lib.json",
        "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
        "build:all": "yarn build:types && yarn build:js",
        "build-lib": "yarn build:clean && yarn build:all && yarn build:post",
        "postinstall.pending": "yarn build-lib",
        "build.current.pending": "yarn build:js"
    },
    "dependencies": {
        "rand-token": "0.4.0",
        "@types/react": "16.8.19",
        "react": "16.8.6",
        "@types/react-native-vector-icons": "6.4.5",
        "i18next": "19.0.3",
        "i18next-browser-languagedetector": "4.0.2",
        "i18next-express-middleware": "1.9.1",
        "i18next-node-fs-backend": "2.1.3",
        "i18next-xhr-backend": "3.2.2",
        "react-i18next": "11.0.0",
        "camelcase": "5.3.1",
        "color": "1.0.3",
        "lodash": "4.17.15",
        "validate.js": "0.13.1",
        "route-parser": "0.0.5",
        "underscore": "1.9.2",
        "axios": "0.19.2",
        "slugify": "1.4.0",
        "moment": "2.24.0",
        "query-string": "6.11.1",
        "numeral": "2.0.6"
    },
    "peerDependencies": {},
    "devDependencies": {
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "@types/sinon": "7.5.2",
        "sinon": "9.0.0",
        "@types/route-parser": "0.1.3",
        "@types/underscore": "1.9.4",
        "@types/query-string": "6.3.0",
        "@types/node": "13.7.7",
        "@babel/plugin-proposal-decorators": "7.8.3",
        "babel-plugin-module-resolver": "4.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "24.1.0",
        "@types/jest": "24.0.6",
        "jest": "24.1.0",
        "@babel/cli": "7.8.4",
        "@babel/core": "7.8.6",
        "@babel/runtime": "7.4.3",
        "@babel/polyfill": "7.8.3",
        "@babel/preset-env": "7.8.6",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "7.8.3",
        "@babel/plugin-transform-runtime": "7.8.3",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.8.3",
        "@types/react-test-renderer": "16.9.2",
        "react-test-renderer": "16.7.0"
    }
}
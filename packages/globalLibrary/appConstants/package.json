{
  "name": "@app/types",
  "version": "0.6.0",
  "private": false,
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/__types__/index.d.ts",
  "scripts": {
    "rm.assets": "rm -rf @types",
    "ready.assets": "run-s  ready:*",
    "ready:definitive": "cpx \"../../globalLibrary/appDefinitive/@types/**/*.*\" ./@types",
    "test.lib": "jest --env=node --colors --coverage test",
    "fix": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "build:reset": "rm -rf dist",
    "build:clean": "yarn build:reset",
    "build:types": "rm -rf dist/__types__ && tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__\" --source-maps inline",
    "build:all": "yarn build:types && yarn build:js",
    "build-lib": "yarn build:clean && yarn build:all",
    "postinstall.pending": "yarn build-lib",
    "build.current.pending": "yarn build:js"
  },
  "dependencies": {
    "@app/avatar": "*",
    "@app/tools": "*",
    "validate.js": "0.12.0",
    "blueimp-md5": "2.10.0",
    "@types/blueimp-md5": "2.7.0",
    "@types/underscore": "1.8.9",
    "underscore": "1.9.1",
    "slugify": "1.3.4",
    "@types/react": "16.8.19",
    "@types/parse": "2.4.3",
    "moment": "2.23.0"
  },
  "devDependencies": {
    "@types/node": "11.10.4",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "@types/jest": "24.0.6",
    "jest": "24.1.0",
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/runtime": "7.4.3",
    "@babel/polyfill": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@types/react-test-renderer": "16.8.1",
    "react-test-renderer": "16.7.0"
  }
}
{
  "name": "ieatta-web",
  "version": "1.0.0",
  "license": "ISC",
  "description": "Web app using next.js and typescript",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --output-style compressed --include-path scss scss/style.scss public/static/style/index.css",
    "rm.assets": "rm -rf @types",
    "ready.assets": "run-s  ready:*",
    "ready:definitive": "cpx \"../../types/**/*.*\" ./@types",
    "app:compile:web": "rm -rf .check/* && tsc -p tsconfig.check.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --no-semi --single-quote --write '**/*.ts{,x}'",
    "ts-lint:web": "tslint -c tslint.json '**/*.{ts,tsx}'",
    "test.web": "jest --config jest.config.web.js --silent",
    "clean:app": "rm -rf .next/",
    "clean:server": "rm -rf production-server && rm -rf production-cloud",
    "clean:all": "yarn clean:app && yarn clean:server",
    "dev-reset": "yarn build:reset && NODE_ENV=development node production-server/server.js -p $PORT",
    "dev-all": "yarn build && NODE_ENV=development node production-server/server.js -p $PORT",
    "dev-only": "NODE_ENV=development node production-server/server.js -p $PORT",
    "dev": "yarn dev-all",
    "dev-bug": "NODE_ENV=development node  --inspect production-server/server.js -p $PORT",
    "postbuild": "cp -r static .next/",
    "postinstall": "yarn ready.assets && yarn build-css",
    "build:server": "tsc --project tsconfig.server.json",
    "build:server:debug": "tsc --project tsconfig.server.debug.json",
    "build:cloud": "tsc --project tsconfig.cloud.json",
    "build:reset": "yarn clean:all && yarn build:server && yarn build:cloud && next build",
    "build": "next build",
    "start": "NODE_ENV=production node production-server/server.js -p $PORT",
    "deploy": "time yarn deploy:heroku && yarn heroku:logs:tail",
    "deploy:force": "yarn deploy:heroku:force",
    "deploy:heroku": "git push heroku $(git rev-parse --abbrev-ref HEAD):master",
    "deploy:heroku:force": "git push -f heroku $(git rev-parse --abbrev-ref HEAD):master",
    "heroku-postbuild": "yarn build:reset",
    "heroku-start": "yarn start",
    "heroku:bash": "heroku run bash",
    "heroku:logs": "heroku logs",
    "heroku:logs:tail": "heroku logs -t"
  },
